	<Libraries>
		<Library Target="html"><![CDATA[
<script type='text/javascript'>
function vdom_ui_timerStartOnce(ob, interval)
{
	eval("var obj = Obj_" + ob.substring(2, ob.length) + ";");
	if (typeof obj === 'undefined') return;
	if (interval == undefined) return;/*interval = 500;*/
	$q(obj).stopTime();
	$q(obj).oneTime(interval, function(i){
        eval("var freeze = window.timer_" + ob + "_freeze;");
		if (freeze == undefined || parseInt(freeze, 10) == 0) {
			execEventBinded(ob.substring(2, ob.length), 'timechange', {count: i});
/*
			var vdomEvent = new VDOM_Event();
			vdomEvent.srcObj = obj.domCont;
			vdomEvent.name = 'timechange';
			vdomEvent.addParameter('count', i);
			obj.execEvent(vdomEvent);
*/
		}
	});
}
function vdom_ui_timerStart2(ob, interval)
{
	eval("var obj = Obj_" + ob.substring(2, ob.length) + ";");
	if (typeof obj === 'undefined') return;
	if (interval == undefined) return;/*interval = 500;*/
    eval("var timer_id = window.timer_" + ob + ";");
    if (timer_id != undefined) {
        clearInterval(timer_id);
    }
    timer_id = setInterval(
        function(i){
            eval("var freeze = window.timer_" + ob + "_freeze;");
            if (freeze == undefined || parseInt(freeze, 10) == 0) {
                execEventBinded(ob.substring(2, ob.length), 'timechange', {count: i});
            }
        }, interval)
    eval("window.timer_" + ob + "=timer_id;");
}
function vdom_ui_timerStop(ob)
{
    eval("var timer_id = window.timer_" + ob + ";");
    if (timer_id == undefined) return;
    clearInterval(timer_id);
    eval("window.timer_" + ob + "=null;");
}
function vdom_ui_timerFreeze(ob, freeze)
{
    eval("window.timer_" + ob + "_freeze=freeze;");
}
</script>
    ]]></Library>
