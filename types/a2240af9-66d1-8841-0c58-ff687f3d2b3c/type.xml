					<SourceCode>
<![CDATA[var id = this.domCont.id;
eval('var selectionmode = window.selectionmode_'+id+';');
eval('var tristate = window.tristate_'+id+';');
if (selectionmode == 'multi') {
	eval('var keys = window.keys_'+id+';');
	if (tristate) {
		var keys2 = {};
		$j.each(keys, function(i, v){
			keys2[v] = keys[i];
		});
		keys = keys2;
	}
	eval('var dth=$j("#'+id+' table input:checkbox");');
	dth.attr('checked', 'checked').parent().not("th").parent().addClass('row_selected');
	if (tristate) {
		execEventBinded(id.substring(2, id.length), "rowsselected", {keyList: JSON.stringify(keys)});
	} else {
		execEventBinded(id.substring(2, id.length), "rowsselected", {keyList: keys});
	}
}]]>
					</SourceCode>
				</Action>
				<Action Name="selectNone" DisplayName="#Lang(603)" Description="#Lang(503)">
					<SourceCode>
<![CDATA[var id = this.domCont.id;
eval('var selectionmode = window.selectionmode_'+id+';');
eval('var tristate = window.tristate_'+id+';');
if (selectionmode == 'multi') {
	var keys = [];
	if (tristate) var keys = {};
	eval('var dth=$j("#'+id+' table input:checkbox");');
	dth.removeAttr('checked').parent().not("th").parent().removeClass('row_selected');
	if (tristate) {
		execEventBinded(id.substring(2, id.length), "rowsselected", {keyList: JSON.stringify(keys)});
	} else {
		execEventBinded(id.substring(2, id.length), "rowsselected", {keyList: keys});
	}
}]]>
					</SourceCode>
