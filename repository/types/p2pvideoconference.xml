    <![CDATA[import json

class VDOM_p2pvideoconference(VDOM_object):
    def render(self, contents=""):
        WOID = (self.id).replace('-', '_')
        ID = 'o_' + WOID

        DEBUG_INFO = {
          "objname": self.name,
          "objtype": "p2pvideoconference",
          "js_lib": "Peer js, Socket.IO",
          "id": ID,
          "version": self.type.version
        }
        DEBUG_INFO_STRING = " ".join([str(key)+"='"+str(value)+"'" for key, value in DEBUG_INFO.items()])

        STYLES = {
        }
        STYLES_STRING_FORMAT = ";".join([str(key)+":"+str(value) for key, value in STYLES.items()])
        STYLES_STRING = """
          <style>
          .%(id)s
            {
              %(styles)s
            }
          </style>
        """ % { "id":ID, "styles": STYLES_STRING_FORMAT }
        STYLES_STRING = STYLES_STRING.replace(" ", "")

        SCRIPT = """
          <script defer>
            $(function() {
              const conferenceOptions = {
                socketIoHost: '%(host)s',
                socketIoPort: '%(port)s',
                peerJsHost: '%(peer_js_host)s',
                peerJsPort: '%(peer_js_port)s',
                peerJsPath: "/",
                videoWrapperClass: '%(video_wrapper_class)s',
                myStreamClass: "my-video-wrapper",
                //common controls classes
                controlsWrapperClass: "controls-wrapper",
                commonControlClass: "control",
                videoIsDisabledClass: "disabled-video",
                audioIsDisabledClass: "disabled-audio",
                streamIsMutedClass: "muted",
                //volume control classes
                volumeControlClass: "volume-control",
                //mute control classes
                muteControlClass: "mute-control",
                muteControlMutedClass: "muted",
                muteControlUnmutedClass: "unmuted",
                //video control classes
                videoControlClass: "video-control",
                disableVideoClass: "disable-video",
                enableVideoClass: "enable-video",
                //micrphone control classes
                microControlClass: "micro-control",
                disableMicroClass: "disable-micro",
                enableMicroClass: "enable-micro",
              };
              window.connector_%(id)s = new $.P2PVideoConferenceConnector('%(id)s', '%(room_id)s', conferenceOptions);
            });
          </script>
        """ % {
          "id": ID,
          "host": self.host,
          "port": self.port,
          "room_id": self.roomid,
          "peer_js_host": self.peerjshost,
          "peer_js_port": self.peerjsport,
          "video_wrapper_class": self.videowrapperclass
        }

        HTML = """<div {debug_string} class='{id} {classname}'></div>""".format(debug_string=DEBUG_INFO_STRING, classname=self.cssclass, id=ID)

        RESULT = """{}{}{}""".format(STYLES_STRING, HTML, SCRIPT)

        return VDOM_object.render(self, contents=RESULT) 

    def wysiwyg(self, contents=""):
        from scripting.legacy.wysiwyg import get_empty_wysiwyg_value

        image_id = "a9cdfb15-cedd-a794-8a09-801025c6b9f4"
        result = get_empty_wysiwyg_value(self, image_id)

        return VDOM_object.wysiwyg(self, contents=result)
